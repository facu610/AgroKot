<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Actividades.SincronizacionActivity">

    <ProgressBar
        android:id="@+id/progressbar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-7dp"
        android:backgroundTint="@color/primaryColor"
        android:indeterminate="true"
        android:indeterminateTint="@color/primaryLightColor"
        android:max="100"
        android:visibility="gone" />


    <TextView
        android:id="@+id/tv_Sincro_ToSincro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="" />

    <Button
        android:id="@+id/b_PruebaConex"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Probar ConexiÃ³n" />

    <Button
        android:id="@+id/b_SincroFichas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sinc. Fichas" />

    <Button
        android:id="@+id/b_SincroInstr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sinc. Instructores" />

    <Button
        android:id="@+id/b_SincroSocios"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sinc. Socios" />


</LinearLayout>